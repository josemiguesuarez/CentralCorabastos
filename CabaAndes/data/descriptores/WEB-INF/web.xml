<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>TwittSpy</display-name>
 <!-- Servlets -->
 <servlet>
  <servlet-name>ServletRegistro</servlet-name>
  <servlet-class>cabandes.servlets.SvtRegistrar</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletIniciarSesion</servlet-name>
  <servlet-class>cabandes.servlets.SvtIniciarSesion</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServeletCmnUserPrincipal</servlet-name>
  <servlet-class>cabandes.servlets.PrincipalCmnUser</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>existencias</servlet-name>
  <servlet-class>cabandes.servlets.SvtExistencias</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletDarPedidosPendientes</servlet-name>
  <servlet-class>cabandes.servlets.SvtPedidosAProvs</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletDarOfertasDeProveedores</servlet-name>
  <servlet-class>cabandes.servlets.SvtOfertasDeProveedores</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SvtPedidos</servlet-name>
  <servlet-class>cabandes.servlets.SvtPedidos</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletProductosConMayorMovimiento</servlet-name>
  <servlet-class>cabandes.servlets.SvtProductosConMayorMovimiento</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletLocalConMayorVentas</servlet-name>
  <servlet-class>cabandes.servlets.SvtConsultarTiendaConMayorVentas</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletVerBodegas</servlet-name>
  <servlet-class>cabandes.servlets.SvtVerBodegas</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletVerProveedores</servlet-name>
  <servlet-class>cabandes.servlets.SvtVerProveedores</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ServletMovimientos</servlet-name>
  <servlet-class>cabandes.servlets.SvtMovimientos</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>PedidosDeLocalBusqueda</servlet-name>
  <servlet-class>cabandes.servlets.SvtVerPedidosDeUnLocal</servlet-class>
 </servlet>
 <!-- Mapping -->
 <servlet-mapping>
  <servlet-name>ServletRegistro</servlet-name>
  <url-pattern>/registro.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletIniciarSesion</servlet-name>
  <url-pattern>/iniciarSesion.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServeletCmnUserPrincipal</servlet-name>
  <url-pattern>/PrincipalCMNUser.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>existencias</servlet-name>
  <url-pattern>/existencias.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletDarPedidosPendientes</servlet-name>
  <url-pattern>/pedidosAProveedores.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletDarOfertasDeProveedores</servlet-name>
  <url-pattern>/licitaciones.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SvtPedidos</servlet-name>
  <url-pattern>/pedidos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletProductosConMayorMovimiento</servlet-name>
  <url-pattern>/ProductoMayorMovimiento.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletLocalConMayorVentas</servlet-name>
  <url-pattern>/LocalConMayorVentas.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletVerBodegas</servlet-name>
  <url-pattern>/bodegas.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletVerProveedores</servlet-name>
  <url-pattern>/proveedores.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ServletMovimientos</servlet-name>
  <url-pattern>/movimientos.htm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>PedidosDeLocalBusqueda</servlet-name>
  <url-pattern>/PedidosDeLocalBusqueda.htm</url-pattern>
 </servlet-mapping>
 <!-- Welcome files -->
 <welcome-file-list>
  <welcome-file>index.htm</welcome-file>
 </welcome-file-list>
 <!-- Login Config -->
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
